<window title="listbox demo" border="normal">
	When you right-click or left-click the listitem, only that item is selected.
	<separator/>
	<listbox width="250px" multiple="true" checkmark="true">
		<listhead sizable="true">
			<listheader label="name" sort="auto" />
			<listheader label="gender" sort="auto" />
		</listhead>
		<listitem context="popupen">
			<listcell label="Mary" />
			<listcell label="FEMALE" />
		</listitem>
		<listitem context="popupen">
			<listcell label="John" />
			<listcell label="MALE" />
		</listitem>
		<listitem context="popupen">
			<listcell label="Jane" />
			<listcell label="FEMALE" />
		</listitem>
		<listitem context="popupen">
			<listcell label="Henry" />
			<listcell label="MALE" />
		</listitem>
	</listbox>
	<menupopup id="popupen">
		<menuitem label="test" />
		<menuitem label="test2" />
	</menupopup>
</window>
