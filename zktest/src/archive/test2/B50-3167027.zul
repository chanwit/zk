<zk>
	<html><![CDATA[
		<ol>
			<li>Click "add node" button.</li>
			<li>A "new item" shall appear.</li>
		</ol>
	]]></html>
	<button label="add node">
		<attribute name="onClick"><![CDATA[
			Treerow tr = new Treerow();
			item.appendChild(tr);
			tr.appendChild(new Treecell("new Item"));
		]]></attribute>
	</button>
	<tree>
		<treechildren>
			<treeitem id="item"/>
		</treechildren>
	</tree>
</zk>
