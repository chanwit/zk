<zk>
	<html><![CDATA[
		<ul>
			<li>It shall not a line between button and input when combobox in readonly</li>
		</ul>
	]]></html>
	<vbox>
		<combobox id="cb" readonly="true"/>
		<combobox buttonVisible="false" readonly="true"/>
		<button label="buttonVisible" onClick="cb.buttonVisible = !cb.buttonVisible;"/>
		<button label="readonly" onClick="cb.readonly = !cb.readonly;"/>
	</vbox>
</zk>
