<zk xmlns:n="http://www.zkoss.org/2005/zk/native">
<n:h2>[ 1909254 ] Enhance Iframe control like default Iframe tag</n:h2>
	<n:ol>
		<n:li></n:li>
	</n:ol>
<window title="ifram demo" border="normal">
	<iframe id="ife"  scrolling="auto" style="border:2px solid"
		src="http://www.google.com" width="95%" />
	<vbox>
		<button label="has Scroll bar" onClick='ife.scrolling= "yes"' />
		<button label="Not Scroll bar" onClick='ife.scrolling= "no"' />
	</vbox>
</window>

</zk>
