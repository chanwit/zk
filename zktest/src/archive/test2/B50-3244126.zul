<zk>
	<html><![CDATA[
		<ol>
			<li>IE only</li>
			<li>Click the space of scroll bar(not scroll bar arrow).</li>
			<li>The scroll bar shall move to right well.</li>
		</ol>
	]]></html>
	<grid width="400px">
		<columns>
			<column label="column ${each}" forEach="1,2,3,4,5,6,7,8,9" width="200px"/>
		</columns>
		<frozen columns="1"/>
		<rows>
			<row forEach="1,2,3,4,5">
				<label value="item ${each}" forEach="1,2,3,4,5,6,7,8,9"/>
			</row>
		</rows>
	</grid>
</zk>
