<window title="Confirm Close" border="normal" width="200px">
	Press one of the following buttons and reload the page to see what happen.
	<button label="confirm on close">
		<attribute name="onClick">
Clients.confirmClose("Your message has not been sent.\nDiscard your message?");
		</attribute>
	</button>
	<button label="reset confirm" onClick="Clients.confirmClose(null)"/>
</window>
